{
  "name": "covid19india",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://www.covid19india.org/",
  "dependencies": {
    "@juggle/resize-observer": "^3.2.0",
    "@primer/octicons-v2-react": "0.0.0-53e900d",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.2",
    "classnames": "^2.2.6",
    "corejs-typeahead": "^1.3.1",
    "d3-array": "^2.8.0",
    "d3-axis": "^2.0.0",
    "d3-brush": "^2.1.0",
    "d3-fetch": "^2.0.0",
    "d3-format": "^2.0.0",
    "d3-geo": "^2.0.1",
    "d3-interpolate": "^2.0.1",
    "d3-interpolate-path": "^2.2.1",
    "d3-scale": "^3.2.3",
    "d3-scale-chromatic": "^2.0.0",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.0.0",
    "d3-transition": "^2.0.0",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.12",
    "fast-deep-equal": "^3.1.3",
    "i18next": "^19.8.3",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.0.21",
    "immer": "^7.0.14",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-content-loader": "^5.1.4",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.8",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "react-spring": "^8.0.27",
    "react-swipeable": "^6.0.0",
    "react-use": "^15.3.4",
    "react-use-gesture": "^8.0.0",
    "source-map-explorer": "^2.5.0",
    "swr": "^0.3.8",
    "topojson": "^3.0.2",
    "use-dark-mode": "^2.3.1",
    "workerize-loader": "^1.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "npm run compress_maps && cp build/index.html build/404.html && npm run build:sw",
    "compress_maps": "rollup -c",
    "test": "react-scripts test --testPathIgnorePatterns=src/tests/mapAndApiStateNames.test.js",
    "test:map": "react-scripts test src/tests/mapAndApiStateNames.test.js",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix --max-warnings=0 .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|md|html)\"",
    "scss-lint": "sass-lint 'src/**/*.scss' -v -f table --max-warnings 0",
    "scss-lint:fix": "sass-lint-auto-fix 'src/**/*.scss' -d -h -c '.sass-lint-auto-fix.yml'",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build:sw": "node ./src/swBuild.js",
    "start:sw": "http-server ./build -c 0",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "npm run eslint:fix",
      "prettier --write \"**/*.+(js|jsx)\""
    ],
    "*.+(json|yml|yaml|md)": [
      "prettier --write \"**/*.+(json|yml|yaml|md)\""
    ],
    "*.+(css|scss)": [
      "npm run scss-lint:fix",
      "npm run scss-lint"
    ]
  },
  "devDependencies": {
    "@welldone-software/why-did-you-render": "^5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "http-server": "^0.12.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "rollup": "^2.33.1",
    "rollup-plugin-copy": "^3.3.0",
    "sass": "^1.29.0",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.21.2",
    "workbox-build": "^5.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
